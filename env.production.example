# Production переменные окружения
# Скопируйте в .env.production и заполните значения
# cp env.production.example .env.production

# Домен
DOMAIN=your-domain.com

# n8n аутентификация
N8N_USER=admin
N8N_PASSWORD=  # Сгенерируйте: openssl rand -base64 20

# n8n шифрование (ВАЖНО!)
N8N_ENCRYPTION_KEY=  # Сгенерируйте: openssl rand -base64 32

# PostgreSQL пароль
POSTGRES_PASSWORD=  # Сгенерируйте: openssl rand -base64 20

# Email для Let's Encrypt (если используете Caddy)
LETSENCRYPT_EMAIL=your-email@example.com

